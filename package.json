{
  "name": "backend",
  "version": "1.0.0",
  "description": "Projeto RockBurguer Sistema de Web de Delivery",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "export NODE_ENV=DEV && tsc-watch -p tsconfig.json --onSuccess \"node dist/index.js\"",
    "devw": "tsc-watch -p tsconfig.json --onSuccess \"node dist/index.js\"",
    "start": "export NODE_ENV=PROD && node dist/index.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "export NODE_ENV=TEST && $(npm bin)/mocha -r ts-node/register --exit --reporter=spec --recursive src/test/**.spec.ts",
    "tsc": "tsc"
  },
  "author": "Jhony Rodrigues da Costa",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/boom": "^7.3.0",
    "@types/hapi__hapi": "^19.0.3",
    "@types/hapi__joi": "^17.1.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.4",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.1",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.2"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment-business-days": "^1.2.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.10",
    "sequelize-typescript": "^1.1.0",
    "typescript": "^3.9.3",
    "uuid": "^3.3.3",
    "xhelpers-api": "^2.1.3"
  }
}
